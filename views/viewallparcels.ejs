<%- include('partials/header') %>
    <!-- CSS only -->
    <div class="container mt-5">
        <h1>VIEW PARCELS </h1>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
            integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="/dashboard">CMS</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item active">
                        <a class="nav-link" href="/dashboard">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/addparcel">Add Parcel <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/addlocation">Add Location <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/addroute">Add Route <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/updateparcel">Update Parcel <span
                                class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/traveltime">Update Travel Time <span
                                class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/viewallparcels">View Parcels <span
                                class="sr-only">(current)</span></a>
                    </li>
                </ul>
            </div>
        </nav>
        <table class="table table-striped jumbotron-centered mx-auto mt-5" style="width:100%">
            <tr>
                <th>PARCEL ID</th>
                <th>START </th>
                <th>DESTINATION</th>
                <th>DELETE</th>
            </tr>
            <% details.forEach(function(detail){ %>
                <tr>
                    <td>
                        <%=detail.parcelid%>
                    </td>
                    <td>
                        <%=detail.start%>
                    </td>
                    <td>
                        <%=detail.destination%>
                    </td>
                    <td>
                        <form action="/deletebyid" method="POST">
                            <div>
                                <input type="hidden" name="parcelid" value="<%=detail.parcelid%>">
                            </div>
                            <button type="submit" class="btn btn-danger">Delete</button>
                        </form>

                    </td>
                </tr>
                <% }) %>
        </table>
    </div>>
    <%- include('partials/header') %>